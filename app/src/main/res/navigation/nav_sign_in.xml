<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_sign_in"
    app:startDestination="@id/signInSelectFragment">

    <fragment
        android:id="@+id/signInSelectFragment"
        android:name="com.start.STart.ui.auth.signin.SignInSelectFragment"
        android:label="fragment_sign_in_select"
        tools:layout="@layout/fragment_sign_in_select">
        <action
            android:id="@+id/action_signInSelectFragment_to_signInFragment"
            app:destination="@id/signInFragment" />
    </fragment>
    <fragment
        android:id="@+id/signInFragment"
        android:name="com.start.STart.ui.auth.signin.SignInFragment"
        android:label="fragment_sign_in"
        tools:layout="@layout/fragment_sign_in" />
</navigation>